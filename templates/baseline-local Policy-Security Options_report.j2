[Unicode]
Unicode=yes

[System Access]
; Accounts: Rename administrator account
NewAdministratorName = "Administrator"
; Accounts: Rename guest account
NewGuestName = "Guest"
; Accounts: Administrator account status (1=Enabled)
EnableAdminAccount = 1
; Accounts: Guest account status (0=Disabled)
EnableGuestAccount = 0
; Accounts: Limit local account use of blank passwords
LimitBlankPasswordUse = 1

[Registry Values]
; Network security: Min session security for NTLM clients (128-bit)
MACHINE\System\CurrentControlSet\Control\Lsa\MSV1_0\NTLMMinClientSec=4,134217728
; Network security: Min session security for NTLM servers (128-bit)
MACHINE\System\CurrentControlSet\Control\Lsa\MSV1_0\NTLMMinServerSec=4,134217728
; UAC: Run admins in Admin Approval Mode
MACHINE\Software\Microsoft\Windows\CurrentVersion\Policies\System\EnableLUA=4,1
; UAC: Switch to secure desktop for elevation
MACHINE\Software\Microsoft\Windows\CurrentVersion\Policies\System\PromptOnSecureDesktop=4,1
; UAC: Virtualize file/registry write failures
MACHINE\Software\Microsoft\Windows\CurrentVersion\Policies\System\EnableVirtualization=4,1
; Audit: Audit the access of global system objects
MACHINE\System\CurrentControlSet\Control\Lsa\AuditBaseObjects=4,0
; Audit: Audit the use of Backup and Restore privilege
MACHINE\System\CurrentControlSet\Control\Lsa\FullPrivilegeAuditing=4,0

